{
  "name": "seeme",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "husky install",
    "build": "rm -rf ./dist && swc src -d dist",
    "build:tsc": "rm -rf ./dist && tsc",
    "test": "npx prisma db push --accept-data-loss && jest --detectOpenHandles --maxConcurrency 3 --forceExit --maxWorkers=50%",
    "test:unit": "jest --testPathPattern=src/services/tests --detectOpenHandles --forceExit --maxWorkers=50%",
    "test:integrate": "npx prisma db push --accept-data-loss && jest --testPathPattern=src/controllers/tests --detectOpenHandles --forceExit --maxWorkers=50%",
    "test:ci": "NODE_ENV=test npx prisma db push && jest --clearCache && jest --maxConcurrency 3 --runInBand --ci",
    "test:coverage": "jest --detectOpenHandles --forceExit --maxWorkers=50% --coverage",
    "dev": "NODE_ENV=development node ./dist/index.js",
    "start": "tsc-watch --onSuccess \"ts-node src/index.ts\"",
    "start:js": "NODE_ENV=production node dist/index.js",
    "pm2": "pm2 start ecosystem.config.cjs --env production",
    "db:generate": "npx prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "npx prisma db push",
    "lint": "eslint ./src",
    "lint:es:fix": "eslint --ext js --ext ts './' --fix",
    "prettier": "prettier --write src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts --fix",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "author": ";",
  "license": "ISC",
  "dependencies": {
    "@godaddy/terminus": "^4.10.2",
    "@prisma/client": "^4.1.0",
    "@swc/helpers": "^0.4.3",
    "aws-sdk": "^2.1087.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "colors": "^1.4.0",
    "connect-redis": "^6.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "helmet": "^5.0.2",
    "jest-mock-extended": "^2.0.7",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "on-finished": "^2.3.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "redis": "^4.0.4",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.8.0",
    "winston": "^3.4.0",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.218",
    "@swc/jest": "^0.2.22",
    "@types/connect-redis": "^0.0.18",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.4.0",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.11",
    "@types/node": "^17.0.8",
    "@types/on-finished": "^2.3.1",
    "@types/passport": "^1.0.10",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-local": "^1.0.34",
    "@types/redis-mock": "^0.17.1",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^28.1.3",
    "lint-staged": "^12.3.1",
    "prettier": "^2.5.1",
    "prisma": "^4.1.0",
    "redis-mock": "^0.56.3",
    "supertest": "^6.2.2",
    "swagger-jsdoc": "^6.2.2",
    "swagger-ui-express": "^4.5.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
  }
}
